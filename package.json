{
  "name": "@dailykit/anykit",
  "version": "1.0.1",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "siddhant <siddhantk232@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dailykit/anykit"
  },
  "scripts": {
    "compile": "rm -rf dist && tsc",
    "build": "parcel build dist/index.js",
    "build:example": "rm example/anykit.js && cp dist/index.js example/anykit.js && cp dist/index.js.map example/index.js.map",
    "predeploy": "yarn compile && yarn build && yarn build:example",
    "run:example": "http-server example/ -p 8081"
  },
  "devDependencies": {
    "http-server": "^0.12.3",
    "parcel-bundler": "^1.12.4",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "schema-dts": "^0.8.1"
  }
}
